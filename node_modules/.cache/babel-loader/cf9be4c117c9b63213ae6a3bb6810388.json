icon dark @click=\"close\">\r\n        <v-icon> mdi mdi-close </v-icon>\r\n      </v-btn>\r\n      <v-toolbar-title> Your Shopping Cart ! </v-toolbar-title>\r\n    </v-toolbar>\r\n    <v-divider></v-divider>\r\n    <v-container fluid>\r\n      <div v-if=\"countCart === 0\">\r\n        <v-alert outlined color=\"warning\" icon=\"mdi mdi-cart-off\">\r\n          Keranjang Belanja Kosong !\r\n        </v-alert>\r\n      </div>\r\n      <v-list three-line v-if=\"countCart > 0\">\r\n        <template v-for=\"(item, index) in carts\">\r\n          <v-list-item :key=\"'cart' + index\">\r\n            <v-list-item-avatar>\r\n              <v-img :src=\"getImage('/books/' + item.cover)\"></v-img>\r\n            </v-list-item-avatar>\r\n\r\n            <v-list-item-content>\r\n              <v-list-item-title v-html=\"item.name\"></v-list-item-title>\r\n              <v-list-item-subtitle>\r\n                Rp. {{ item.price.toLocaleString(\"id-ID\") }} ({{ item.weight }}\r\n                kg)\r\n                <span style=\"float: right\">\r\n                  <v-btn\r\n                    icon\r\n                    small\r\n                    rounded\r\n                    depressed\r\n                    @click.stop=\"removeCart(item)\"\r\n                  >\r\n                    <v-icon dark color=\"error\"> mdi mdi-minus-circle </v-icon>\r\n                  </v-btn>\r\n                  {{ item.quantity }}\r\n                  <v-btn\r\n                    icon\r\n                    small\r\n                    rounded\r\n                    depressed\r\n                    @click.stop=\"addCart(item)\"\r\n                  >\r\n                    <v-icon dark color=\"success\"> mdi mdi-plus-circle </v-icon>\r\n                  </v-btn>\r\n                </span>\r\n              </v-list-item-subtitle>\r\n            </v-list-item-content>\r\n          </v-list-item>\r\n        </template>\r\n      </v-list>\r\n      <v-card>\r\n        <v-card-text>\r\n          <v-layout wrap>\r\n            <v-flex pa-1 xs6>\r\n              Total Price ({{ totalQuantity }} items) <br />\r\n              <span class=\"title\">\r\n                Rp. {{ totalPrice.toLocaleString(\"id-ID\") }}\r\n              </span>\r\n            </v-flex>\r\n            <v-flex pa-1 xs6 text-right>\r\n              <v-btn\r\n                color=\"primary\"\r\n                @click=\"checkout\"\r\n                :disabled=\"totalQuantity == 0\"\r\n              >\r\n                <v-icon> mdi mdi-cart-arrow-right</v-icon> &nbsp; Checkout\r\n              </v-btn>\r\n            </v-flex>\r\n          </v-layout>\r\n        </v-card-text>\r\n      </v-card>\r\n    </v-container>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\";\r\nexport default {\r\n  name: \"cart\",\r\n  computed: {\r\n    ...mapGetters({\r\n      carts: \"cart/carts\",\r\n      countCart: \"cart/count\",\r\n      totalPrice: \"cart/totalPrice\",\r\n      totalQuantity: \"cart/totalQuantity\",\r\n      totalWeight: \"cart/totalWeight\",\r\n    }),\r\n  },\r\n  methods: {\r\n    ...mapActions({\r\n      setStatusDialog: \"dialog/setStatus\",\r\n      setAlert: \"alert/set\",\r\n      addCart: \"cart/add\",\r\n      removeCart: \"cart/remove\",\r\n      setCart: \"cart/set\",\r\n    }),\r\n    checkout() {\r\n      this.$emit(\"closed\", false);\r\n      this.$router.push({ path: \"/checkout\" });\r\n    },\r\n    close() {\r\n      this.setStatusDialog(false);\r\n    },\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}